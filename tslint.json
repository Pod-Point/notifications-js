{
  "extends": "@pod-point/tslint-config-podpoint-base",
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "ordered-imports": [
      true
    ]
  }
}
